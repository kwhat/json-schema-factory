{
    "name": "aicconsultinginc/Bildr-JsonSchema",
    "type": "library",
    "description": "JSON Schema Generator.",
    "license": "unknown",
    "authors": [
        {
            "name": "Hunter Elbourn",
            "role": "Developer",
            "homepage": "https://github.com/HunterEl"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "doctrine/common": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"

    },
    "autoload": {
        "psr-4": {
